    <DataSet Name="Downtime">
      <Query>
        <DataSourceName>CIMCO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@workcenter">
            <Value>=Parameters!workcenter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@machineno">
            <Value>=Parameters!machineno.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@downtimecode">
            <Value>=Parameters!downtimecode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH downtime AS 
	(
	SELECT        
		Workcenter, 
		MachineID, 
		MachineNo, 
		MachineDescription,
		StartTime,
		CASE
			WHEN StartTime &lt; @from THEN @from
			ELSE StartTime
		END AS StartTimeCorrect,
		EndTime,
		CASE 
			WHEN EndTime &gt; @to THEN @to 
			ELSE EndTime 
		END AS EndTimeCorrect,
		DownTimeCode, 
		DownTimeDescription
	FROM
		[dbo].[vw_downtime]
	WHERE
		[Status] &lt;&gt; 'LOGGED'
		AND
		Workcenter IN (@workcenter)
		AND
		MachineNo IN (@machineno)
		AND
		DownTimeCode IN (@downtimecode)
	)
SELECT
	Workcenter,
	MachineNo,
	MachineDescription,
	DownTimeCode, 
	DownTimeDescription,
	SUM(DATEDIFF(s, StartTimeCorrect, EndTimeCorrect)) AS DowntimeCorrect
FROM
	downtime
WHERE
	StartTimeCorrect BETWEEN @from AND @to
	AND
	EndTimeCorrect BETWEEN @from AND @to
GROUP BY
	Workcenter,
	MachineNo,
	MachineDescription,
	DownTimeCode, 
	DownTimeDescription
ORDER BY
	Workcenter,
	MachineNo,
	DownTimeCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Workcenter">
          <DataField>Workcenter</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MachineNo">
          <DataField>MachineNo</DataField>
