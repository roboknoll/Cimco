    <DataSet Name="DowntimeGroupBy">
      <Query>
        <DataSourceName>CIMCO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@workcenter">
            <Value>=Parameters!workcenter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@machineno">
            <Value>=Parameters!machineno.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@downtimecode">
            <Value>=Parameters!downtimecode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@groupby">
            <Value>=Parameters!groupby.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH downtime AS 
	(
	SELECT        
		Workcenter, 
		MachineID, 
		MachineNo, 
		MachineDescription,
		StartTime,
		CASE
			WHEN StartTime &lt; @from THEN @from
			ELSE StartTime
		END AS StartTimeCorrect,
		EndTime,
		CASE 
			WHEN EndTime &gt; @to THEN @to 
			ELSE EndTime 
		END AS EndTimeCorrect,
		DownTimeCode, 
		DownTimeDescription
	FROM
		[dbo].[vw_downtime]
	WHERE
		[Status] &lt;&gt; 'LOGGED'
		AND
		Workcenter IN (@workcenter)
		AND
		MachineNo IN (@machineno)
		AND
		DownTimeCode IN (@downtimecode)
	)
 	SELECT
		CASE 
			WHEN @groupby = 'workcenter' THEN Workcenter
			WHEN @groupby = 'machineno' THEN MachineNo
			WHEN @groupby = 'downtimecode' THEN DownTimeCode
		END AS [GroupBy],
		SUM(DATEDIFF(s, StartTimeCorrect, EndTimeCorrect)) AS DowntimeCorrect
	FROM
		downtime
	WHERE
		StartTimeCorrect BETWEEN @from AND @to
		AND
		EndTimeCorrect BETWEEN @from AND @to
	GROUP BY
		CASE 
			WHEN @groupby = 'workcenter' THEN Workcenter
			WHEN @groupby = 'machineno' THEN MachineNo
			WHEN @groupby = 'downtimecode' THEN DownTimeCode
		END</CommandText>
      </Query>
      <Fields>
        <Field Name="groupby">
          <DataField>GroupBy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DowntimeCorrect">
          <DataField>DowntimeCorrect</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
